    "@angular/localize": "^19.2.14",
    "@types/jasmine": "~5.1.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "typescript": "~5.7.2"
